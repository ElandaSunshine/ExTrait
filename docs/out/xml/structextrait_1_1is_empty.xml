  <compounddef id="structextrait_1_1is_empty" kind="struct" language="C++" prot="public">
    <compoundname>extrait::isEmpty</compoundname>
    <basecompoundref refid="structextrait_1_1detail_1_1is_empty" prot="public" virt="non-virtual">extrait::detail::isEmpty&lt; T &gt;</basecompoundref>
