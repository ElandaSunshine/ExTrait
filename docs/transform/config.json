{
    "workingDir": "../out/xml/",
    "outputDir": "../out/trans/",
    "entities": [
        "class", "struct", "union",
        "function",
        "variable",
        "typedef"
    ],
    "ignoredFields": [
        "bases", "visibility", "properties.qualifiers", "properties.virtual", "properties.noexcept",
        "properties.inline", "properties.explicit", "properties.constexpr", "params"
    ],
    "documents": [
        {
            "out": "common.json",
            "query": {
                "groups": [ "common" ]
            }
        },
        {
            "out": "type-list.json",
            "query": {
                "groups": [ "type-list" ]
            }
        },
        {
            "out": "reflection.json",
            "query": {
                "groups": [ "reflection" ]
            }
        }
    ]
}
